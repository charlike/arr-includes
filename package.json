{
  "name": "arr-includes",
  "version": "1.0.2",
  "description": "Return true if any of passed values exists in array. Using [in-array][].",
  "repository": "tunnckoCore/arr-includes",
  "author": "Charlike Mike Reagent <@tunnckoCore> (http://www.tunnckocore.tk)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard --verbose",
    "pretest": "npm run lint",
    "test": "npm run coverage",
    "posttest": "npm run lint:coverage",
    "coverage": "nyc node test.js",
    "lint:coverage": "nyc check-coverage --lines 100 --branches 100 --statements 100",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "in-array": "^0.1.2",
    "isarray": "^2.0.0",
    "lazy-cache": "^2.0.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.14",
    "mukla": "^0.4.4",
    "nyc": "^8.3.0",
    "pre-commit": "^1.1.3",
    "standard": "^8.1.0"
  },
  "files": [
    "index.js",
    "utils.js"
  ],
  "keywords": [
    "arr",
    "arr-includes",
    "array",
    "check",
    "exists",
    "in",
    "in-array",
    "includes",
    "pass",
    "validate",
    "value",
    "values"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "in-array",
        "isarray",
        "is-missing",
        "is-installed",
        "is-async-function"
      ]
    },
    "reflinks": [
      "in-array"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
